{
    "JwtSettings": {
        "Secret": "<YOUR_JWT_SECRET_MIN_32_CHARS>",
        "Issuer": "SakaeAPI",
        "Audience": "SakaeClient",
        "ExpirationMinutes": 60,
        "RefreshTokenExpirationDays": 7
    },
    "ConnectionStrings": {
        "DefaultConnection": "Server=<YOUR_DB_SERVER>;Initial Catalog=<YOUR_DB_NAME>;User ID=<YOUR_DB_USER>;Password=<YOUR_DB_PASSWORD>;Encrypt=True;TrustServerCertificate=False;"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Authentication": {
        "Google": {
            "ClientId": "<YOUR_GOOGLE_CLIENT_ID>",
            "ClientSecret": "<YOUR_GOOGLE_CLIENT_SECRET>"
        }
    }
}